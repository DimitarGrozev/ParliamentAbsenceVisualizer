{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://www.parliament.bg/api/:splat",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/images/*",
      "rewrite": "https://www.parliament.bg/images/:splat",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/images/*"]
  }
}
